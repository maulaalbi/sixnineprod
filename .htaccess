RewriteEngine On

# Redirect URL dengan .html ke URL tanpa .html
RewriteCond %{REQUEST_URI} ^/(.*)\.html$ [NC]
RewriteRule ^ %1 [R=301,L]

# Menyajikan file .html untuk URL tanpa ekstensi
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^(.*)$ $1.html [L]
RewriteCond %{HTTP_HOST} ^www\.sewaalatpesta69pro\.com [NC]
RewriteRule ^(.*)$ https://sewaalatpesta69pro.com/$1Â [L,R=301]

